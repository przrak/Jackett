{
  "name": "Jacket",
  "description": "Jackett on heroku with saved config",
  "repository": "https://github.com/amjiddader/Jackett",
  "keywords": ["docker", "jackett", "rss", "heroku", "cloudflare"],
  "stack": "container",
  "success_url": "/",
  "env": {
    "ENABLE_AUTH": {
      "description": "Jackett Admin Password not required",
      "value": "admin",
      "required": false
    },
    "FLARE": {
      "description": "Cloudflare Verify Url",
      "value": "added_in_config",
      "required": false
    }
  }
}
